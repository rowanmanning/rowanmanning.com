---
layout:
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en-GB">

    <title>Rowan Manning's Blog</title>
    <link href="https://rowanmanning.com{{ page.url }}" rel="self"/>
    <link href="https://rowanmanning.com/"/>
    <updated>{{ site.time | date: '%Y-%m-%dT%H:%M:%SZ' }}</updated>
    <id>https://rowanmanning.com{{ page.url }}</id>
    <author>
        <name>Rowan Manning</name>
        <uri>https://rowanmanning.com/</uri>
    </author>

    {% for post in site.posts %}
        {% if post.sitemap != false %}
            <entry>
                <title>{{ post.title }}</title>
                <link href="https://rowanmanning.com{{ post.url }}"/>
                <published>{{ post.date | date: '%Y-%m-%dT%H:%M:%SZ' }}</published>
                <updated>
                    {% if post.lastmod %}
                        {{ post.lastmod | date: '%Y-%m-%dT%H:%M:%SZ' }}
                    {% else %}
                        {{ post.date | date: '%Y-%m-%dT%H:%M:%SZ' }}
                    {% endif %}
                </updated>
                <id>https://rowanmanning.com{{ post.url }}</id>
                <content type="html">{{ post.content | xml_escape }}</content>
            </entry>
        {% endif %}
    {% endfor %}

</feed>
